    style.
      #background {
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background-image: url('https://upload.wikimedia.org/wikipedia/commons/6/6b/A._S._Bradford_House.JPG');
      background-repeat: no-repeat;
      background-attachment: fixed;
      background-size: cover;
      opacity: 0.2;
      filter:alpha(opacity=40);
      z-index: -1;
      }
      #FeatureHouseSection {
      height: 650px;
      background-size: cover;
      }
      #FeatureContactBox {
      position: absolute;
      background-color: rgb(0, 51, 102);
      width: 200px;
      height: 70px;
      color: white;
      right: 10%;
      opacity: 0.9;
      }
      #FeatureContactBox:hover {
      filter: drop-shadow(8px 8px 10px black);
      }
      #FeatureDescriptionBox {
      position: absolute;
      width: 150px;
      height: 100px;
      right: 60%;
      top: 200px;
      text-align: center;
      }
      #FeatureDescriptionBoxOpacity {
      position: absolute;
      background-color: rgb(0, 51, 102);
      filter: drop-shadow(8px 8px 10px black);
      width: 500px;
      height: 150px;
      color: white;
      opacity: 0.8;
      z-index: 0;
      }
      #FeatureDescriptionInfo {
      text-align: center;
      position: relative;
      color: white;
      z-index: 1;
      }
      #FeatureDescription{
      text-align: center;
      z-index: 1;

      }
      #BlankSection1, #BlankSection2 {
      height: 150px;
      background-color: transparent;
      }
      #NewHousesSection {
      background-color:transparent;
      }
      .NewHousesBox {
      position:relative;
      display: inline-block;
      background-color: white;
      height: 350px;
      width:300px;
      background-size: cover;
      opacity: 1;
      transition: .5s ease;
      backface-visibility: hidden;
      }
      .NewHousesBox:hover {
      opacity: 0.5;
      }
      .MiddleBox {
      transition: .5s ease;
      opacity: 0;
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      -ms-transform: translate(-50%, -50%)
      }
      .NewHousesBox:hover .MiddleBox {
      opacity: 1;
      }
      .text {
      background-color: #4CAF50;
      color: white;
      font-size: 16px;
      padding: 16px 32px;
      }
      #TopRatingHousesSection1 {
      height: 400px;
      background-color: rgb(0, 51, 102);
      }
      #RatingHouse1Img {
      position: absolute;
      height: 350px;
      width: 30%;
      right: 60%;
      top: 1000px;
      background-size: cover;
      }
      #RatingHouse1Img:hover {
      filter: drop-shadow(8px 8px 10px black);
      }
      #RatingHouse1DescriptionBox {
      position: relative;
      height: 275px;
      width: 35%;
      left: 50%;
      top: 10%;
      color: white;
      text-align: center;
      border-top: 2px dashed #046ca3;
      border-bottom: 1px dashed #046ca3;
      box-shadow: inset 0 -1px 0 0 #046ca3, inset 0 1px 0 0 #046ca3, 0 1px 0 0 #046ca3, 0 -1px 0 0 #046ca3;
      margin-bottom: 1px;
      }
      #TopRatingHousesSection2 {
      height: 400px;
      background-color: rgb(0, 70, 111);
      }
      #RatingHouse2Img {
      position: absolute;
      height: 350px;
      width: 30%;
      right: 15%;
      top: 1525px;
      background-size: cover;
      }
      #RatingHouse2Img:hover {
      filter: drop-shadow(8px 8px 10px black);
      }
      #RatingHouse2DescriptionBox {
      position: relative;
      height: 275px;
      width: 35%;
      left: 10%;
      top: 10%;
      color: white;
      text-align: center;
      border-top: 2px dashed #046ca3;
      border-bottom: 1px dashed #046ca3;
      box-shadow: inset 0 -1px 0 0 #046ca3, inset 0 1px 0 0 #046ca3, 0 1px 0 0 #046ca3, 0 -1px 0 0 #046ca3;
      margin-bottom: 1px;
      }
      #TitleText {
      border: 5px solid #900;
      border-top: none;
      border-left: none;
      width: 500px;
      margin: 0 auto;
      border-color: #004080;
      color: #004080;
      }
